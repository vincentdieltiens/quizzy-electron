{
  "name": "quizzy-game",
  "description": "Desktop application game for quizzy",
  "version": "0.1.0",
  "author": "Vincent Dieltiens",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentdieltiens/quizzy-game"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "bootstrap": "^3.3.7",
    "electron": "^1.7.10",
    "font-awesome": "^4.7.0",
    "fs-extra": "^1.0.0",
    "howler": "^2.0.2",
    "inherits": "^2.0.3",
    "ip": "^1.1.5",
    "node": "^9.3.0",
